- if location.file_uploads.present?
  %section.location-sidebar-content.file_upload
    %ul
      - location.file_uploads.each do |upload|
        %li
          %span
            - url = ENV['OHANA_API_ENDPOINT'].gsub('/api', '') + upload[:url]
            %a{:href => url, :target => "_blank"}= "#{upload.filename}"
