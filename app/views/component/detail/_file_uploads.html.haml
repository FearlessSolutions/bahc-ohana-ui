- if location.file_uploads.present?
  %section.location-sidebar-content.file_upload
    %ul
      - location.file_uploads.each do |upload|
        %li
          %span
            %a{:href => get_pdf_file_link(decode_json(upload.image_data)["id"]), :target => "_blank"}= "#{get_pdf_filename(upload.image_data)}"